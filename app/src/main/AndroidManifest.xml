<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="afinal.proyecto.cuatro.grupo.airportsindoorlocationapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.SignInActivity" />
        <activity android:name=".activities.UserSupportActivity" />
        <activity android:name=".activities.HomeActivity" />
        <activity android:name=".activities.VueloActivity" />
        <activity android:name=".activities.MapaActivity" />
        <activity android:name=".activities.DestinoActivity" />
        <activity android:name=".activities.InfoActivity" />
        <activity android:name=".activities.CargarVueloActivity" />

    </application>

    </manifest>